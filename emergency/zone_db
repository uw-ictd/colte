$TTL	604800
@ IN  SOA colte.emergency. admin.emergency. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

 IN  NS  colte.emergency.

colte.emergency.  IN  A 192.168.151.1
home.emergency.  IN  A 192.168.151.1

