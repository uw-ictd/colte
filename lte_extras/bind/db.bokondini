@	IN	SOA	colte.bokondini. admin.bokondini. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

	IN	NS	colte.bokondini.

colte.bokondini.	IN	A	192.168.151.1
network.bokondini.	IN	A	192.168.151.1


