;
; BIND data file for local loopback interface
;
$TTL	604800
@ IN  SOA colte.bokondini. admin.bokondini. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

 IN  NS  colte.bokondini. ;N1

colte.bokondini.  IN  A 192.168.151.1 ;A1
network.bokondini.  IN  A 192.168.151.1 ;A2
media.bokondini.  IN  A 192.168.151.1
maps.bokondini.  IN  A 192.168.151.1
xowa.bokondini.  IN  A 192.168.151.1
wiki.bokondini.  IN  A 192.168.151.1
chat.bokondini.  IN  A 192.168.151.1

